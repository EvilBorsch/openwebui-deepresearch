# Example environment file for Open WebUI Remote Tools Server

# Core tokens
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CX=your_google_cse_id

# Server
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
CORS_ORIGINS=*

# Browser
BROWSER_HEADLESS=true
BROWSER_TIMEOUT_SECONDS=35
BROWSER_LOCALE=en-US
BROWSER_TIMEZONE_ID=UTC
BROWSER_MAX_SCROLLS=8
BROWSER_SCROLL_PAUSE_MS=400
BROWSER_USER_AGENT=
# Proxies (optional)
HTTP_PROXY=
HTTPS_PROXY=
BROWSER_PROXY=

# Rate limit
PAGE_TOOL_LIMIT=20
SESSION_TTL_SECONDS=3600

# Optional .env path override
ENV_FILE=.env
